@{
    ViewBag.Title = "Clientes";
    
}

<h3>Clientes</h3>

<table class="table table-hover">
    <tr>
        <th scope="col">Cod</th>
        <th scope="col">Segurado</th>
        <th scope="col">CNPJ</th>
     
        <th scope="col">Celular</th>
        <th scope="col">Apolices</th>


    </tr>

    @{ foreach (var item in ViewBag.lista)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Segurado</td>
            <td>@item.CNPJ</td>
            <td>@item.Celular</td>
            
            <td>@item.Apolices</td>

        </tr>        
            }

    }
</table>
